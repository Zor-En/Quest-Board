{
    "version": 2,
    "builds": [
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/(.*)",
        "use": "@vercel/static",
        "config": {
          "public": "client",
          "distDir": "client"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "client/index.html"
      }
    ]
  }