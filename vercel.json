{
    "version": 2,
    "builds": [
        {
            "src": "client/(.*)",
            "use": "@vercel/static",
            "config": {
              "public": "client",
              "distDir": "client"
            }
          },
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "client/index.html"
      }
    ]
  }